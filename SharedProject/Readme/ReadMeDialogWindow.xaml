<platform:DialogWindow  x:Class="FineCodeCoverage.Readme.ReadMeDialogWindow"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                        xmlns:platform="clr-namespace:Microsoft.VisualStudio.PlatformUI;assembly=Microsoft.VisualStudio.Shell.15.0"
                        xmlns:fccwpf="clr-namespace:FineCodeCoverage.Wpf"
                        xmlns:markdig="clr-namespace:Markdig.Wpf;assembly=Markdig.Wpf"
                        mc:Ignorable="d"
                        Width="600"
                        Height="400"
                        d:DesignHeight="600"
                        d:DesignWidth="400"
						
						>
    <FrameworkElement.CommandBindings>        <CommandBinding Command="{x:Static markdig:Commands.Hyperlink}" Executed="OpenHyperlink" />        <CommandBinding Command="{x:Static markdig:Commands.Image}" Executed="ClickOnImage" />    </FrameworkElement.CommandBindings>
    <markdig:MarkdownViewer x:Name="Viewer"/>
</platform:DialogWindow>
